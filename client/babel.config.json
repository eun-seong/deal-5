{
  "presets": [
    [
    "@babel/typescript",
      {
        "targets": "> 1%, not dead",
        "useBuiltIns": "usage",
        "corejs": { "version": 3, "proposals": true }
      }
    ]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    ["babel-plugin-root-import", {"rootpathPrefix" : "@/", "rootPathSuffix" :"./"}]
  ]
}
